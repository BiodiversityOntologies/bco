###  http://purl.obolibrary.org/obo/RO_0002151

obo:RO_0002151 rdf:type owl:ObjectProperty ;
               
               rdfs:label "proper overlaps"@en ;
               
               obo:IAO_0000426 """(forall (?x ?y) 
	(iff 
		(proper_overlaps ?x ?y)
		(and 
			(overlaps ?x ?y)
			(not (part_of ?x ?y)) 
			(not (part_of ?y ?x)))))"""^^xsd:string ;
               
               obo:IAO_0000118 "partially overlaps"^^xsd:string ;
               
               obo:IAO_0000114 obo:IAO_0000125 ;
               
               obo:RO_0001900 obo:RO_0001901 ;
               
               rdfs:subPropertyOf obo:RO_0002131 .



