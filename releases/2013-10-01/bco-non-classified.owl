        <obo:RO_0001900 rdf:resource="&obo;RO_0001901"/>
        <rdfs:subPropertyOf rdf:resource="&obo;RO_0002323"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&obo;BFO_0000051"/>
