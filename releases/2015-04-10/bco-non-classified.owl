        <obo:IAO_0000115>A planned process whereby a person submits a material sample to an organization.</obo:IAO_0000115>
        <dc:creator>Biocode Commons Ontology Hackathon</dc:creator>
        <obo:IAO_0000112 xml:lang="en">a curator submitting a herbarium speciman to a museum</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">a researcher submitting a water sample to a laboratory storage collection</obo:IAO_0000112>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
